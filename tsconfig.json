{
   "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "outDir": "./dist",
      "module": "commonjs",
      "target": "es2019",
      "esModuleInterop": true,
      "sourceMap": true,
      "rootDirs": [
         "src",
      ],
      "baseUrl": "src",
      "paths": {
         "@/*": [
            "*"
         ],
         "@/models/*": [
            "/app/models/*"
         ]
      }
   },
   "include": [
      "src",
   ],
   "exclude": []
}