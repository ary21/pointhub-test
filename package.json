{
  "name": "pointhub-test",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:ary21/pointhub-test.git",
  "author": "Eko Nur Arifin <eko@orenda.co.id>",
  "license": "MIT",
  "scripts": {
    "watch": "concurrently -k \"npm run watch-ts\" \"npm run watch-node\"",
    "start": "node -r dotenv/config dist/server.js",
    "build": "yarn run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon -r dotenv/config dist/server.js"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.3",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
